# API Configuration for Finvinity
# Copy this file to .env and add your actual API keys

# NewsAPI.org - Free tier: 1,000 requests/day
# Get your key at: https://newsapi.org/register
EXPO_PUBLIC_NEWS_API_KEY=your_newsapi_key_here

# Finnhub.io - Free tier: 60 calls/minute
# Get your key at: https://finnhub.io/register  
EXPO_PUBLIC_FINNHUB_API_KEY=your_finnhub_key_here

# Alpha Vantage - Free tier: 25 requests/day
# Get your key at: https://www.alphavantage.co/support/#api-key
EXPO_PUBLIC_ALPHA_VANTAGE_API_KEY=your_alphavantage_key_here

# Twelve Data - Free tier: 800 requests/day  
# Get your key at: https://twelvedata.com/pricing
EXPO_PUBLIC_TWELVE_DATA_API_KEY=your_twelvedata_key_here

# Development settings
EXPO_PUBLIC_DEBUG_LOGGING=true
EXPO_PUBLIC_ENABLE_REAL_APIS=true 